<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite" parallel = "tests">
<listeners>
<listener class-name="Acacemy.Listeners"/>
</listeners>
   <test name = "Addition">
    <classes>
     <class name="Acacemy.additionTest"/>
    </classes>
   </test>
   <test name = "Subtraction">
    <classes>
     <class name="Acacemy.SubtractionTest"/>
    </classes>
   </test>
   <test name = "Multiplication">
    <classes>
     <class name="Acacemy.multiplicationTests"/>
    </classes>
   </test>
   <test name = "Division">
    <classes>
     <class name="Acacemy.DivisionTests"/>
    </classes>
   </test>
   <test name = "Verifying Elements">
    <classes>
     <class name="Acacemy.VerifyingElementsareClickable"/>
    </classes>
   </test>
</suite> <!-- Suite -->
